<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Zayso\Osso2007Bundle\Entity\PhoneDoctrine" table="phone_doctrine">
    <change-tracking-policy>DEFERRED_IMPLICIT</change-tracking-policy>
    <id name="phoneId" type="bigint" column="phone_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="phoneTypeId" type="bigint" column="phone_type_id"/>
    <field name="personId" type="bigint" column="person_id"/>
    <field name="areaCode" type="string" column="area_code" length="4"/>
    <field name="num" type="string" column="num" length="8"/>
    <field name="ext" type="string" column="ext" length="8"/>
    <lifecycle-callbacks/>
  </entity>
</doctrine-mapping>
