<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Zayso\Osso2007Bundle\Entity\EventPerson" table="event_person">
    <change-tracking-policy>DEFERRED_IMPLICIT</change-tracking-policy>
    <id name="eventPersonId" type="integer" column="event_person_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="eventId" type="integer" column="event_id"/>
    <field name="personId" type="integer" column="person_id"/>
    <field name="eventPersonTypeId" type="integer" column="event_person_type_id"/>
    <field name="regYearId" type="integer" column="reg_year_id"/>
    <field name="status" type="integer" column="status"/>
    <lifecycle-callbacks/>
  </entity>
</doctrine-mapping>
