<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Zayso\Osso2007Bundle\Entity\Openid" table="openid">
    <change-tracking-policy>DEFERRED_IMPLICIT</change-tracking-policy>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="accountId" type="integer" column="account_id"/>
    <field name="memberId" type="integer" column="member_id"/>
    <field name="personId" type="integer" column="person_id"/>
    <field name="status" type="integer" column="status"/>
    <field name="displayName" type="string" column="display_name" length="80"/>
    <field name="userName" type="string" column="user_name" length="80"/>
    <field name="email" type="string" column="email" length="80"/>
    <field name="provider" type="string" column="provider" length="80"/>
    <field name="identifier" type="string" column="identifier" length="120"/>
    <lifecycle-callbacks/>
  </entity>
</doctrine-mapping>
