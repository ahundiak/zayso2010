<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Zayso\Osso2007Bundle\Entity\Vol" table="vol">
    <change-tracking-policy>DEFERRED_IMPLICIT</change-tracking-policy>
    <id name="volId" type="integer" column="vol_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="personId" type="integer" column="person_id"/>
    <field name="volTypeId" type="integer" column="vol_type_id"/>
    <field name="regYearId" type="integer" column="reg_year_id"/>
    <field name="seasonTypeId" type="integer" column="season_type_id"/>
    <field name="unitId" type="integer" column="unit_id"/>
    <field name="divisionId" type="integer" column="division_id"/>
    <field name="note" type="string" column="note" length="40"/>
    <field name="regForm" type="integer" column="reg_form"/>
    <field name="refresher" type="integer" column="refresher"/>
    <field name="certified" type="integer" column="certified"/>
    <field name="uniform" type="string" column="uniform" length="40"/>
    <field name="equipment" type="string" column="equipment" length="40"/>
    <lifecycle-callbacks/>
  </entity>
</doctrine-mapping>
