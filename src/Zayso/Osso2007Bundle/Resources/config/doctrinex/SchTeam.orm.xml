<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Zayso\Osso2007Bundle\Entity\SchTeam" table="sch_team">
    <change-tracking-policy>DEFERRED_IMPLICIT</change-tracking-policy>
    <id name="schTeamId" type="integer" column="sch_team_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="phyTeamId" type="integer" column="phy_team_id"/>
    <field name="projectId" type="integer" column="project_id"/>
    <field name="regYearId" type="integer" column="reg_year_id"/>
    <field name="seasonTypeId" type="integer" column="season_type_id"/>
    <field name="scheduleTypeId" type="integer" column="schedule_type_id"/>
    <field name="unitId" type="integer" column="unit_id"/>
    <field name="divisionId" type="integer" column="division_id"/>
    <field name="sortx" type="integer" column="sortx"/>
    <field name="descShort" type="string" column="desc_short" length="20"/>
    <lifecycle-callbacks/>
  </entity>
</doctrine-mapping>
