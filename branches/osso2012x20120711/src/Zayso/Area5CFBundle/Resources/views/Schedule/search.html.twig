<div>
<form method="post" action="{{ path(postPath) }}">
<table border="1" class="form_table" width="800">
    <tr><th colspan="1">Search Game Schedule</th></tr>
    <tr><td>
<table border="1"><tr><td>
      From:
      <select name="schedSearchData[date1]">
        {{ format.formOptions(datePickList,schedSearchData.date1) | raw }}
      </select>
</td><td>
      To:
      <select name="schedSearchData[date2]">
        {{ format.formOptions(datePickList,schedSearchData.date2) | raw }}
      </select>
</td><td>
       Sort By:
      <select name="schedSearchData[sortBy]">
        {{ format.formOptions(sortByPickList,schedSearchData.sortBy) | raw }}
      </select>
</td><td width="200" align="center">
       Game Count: {{ gameCount }}
</td></tr></table>
    </td></tr>
    <tr><td>
        <table border="1" style="margin: 0;"><tr>
        {% for region in regions %}
            <td width="75" align="center">{{ gen.genRegionCheckBox(region) | raw }}</td>
        {% endfor %}
        </tr></table>
    </td></tr>
    <tr><td>
<table><tr><td>
        <table border="1" style="margin: 0;"><tr>
        {% for age in ages %}
            <td width="40" align="center">{{ gen.genAgeCheckBox(age) | raw }}</td>
        {% endfor %}
        </tr></table>
</td><td>
        <table border="1" style="margin: 0;"><tr>
        {% for gender in genders %}
            <td width="40" align="center">{{ gen.genGenderCheckBox(gender) | raw }}</td>
        {% endfor %}
        </tr></table>
</td></tr></table>
     </td></tr>
    <tr>
        <td align="center"><input type="submit" name="schedSearchSubmit" value="Search" /></td>
    </tr>
</table>
</form>
</div>
<div>
