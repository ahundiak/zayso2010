<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Zayso\Osso2007Bundle\Entity\RefAvail" table="ref_avail">
    <change-tracking-policy>DEFERRED_IMPLICIT</change-tracking-policy>
    <id name="refAvailId" type="integer" column="ref_avail_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="groupId" type="integer" column="group_id"/>
    <field name="personId" type="integer" column="person_id"/>
    <field name="divCrId" type="integer" column="div_cr_id"/>
    <field name="divArId" type="integer" column="div_ar_id"/>
    <field name="phoneHome" type="string" column="phone_home" length="40"/>
    <field name="phoneWork" type="string" column="phone_work" length="40"/>
    <field name="phoneCell" type="string" column="phone_cell" length="40"/>
    <field name="emailHome" type="string" column="email_home" length="40"/>
    <field name="emailWork" type="string" column="email_work" length="40"/>
    <field name="availDay1" type="integer" column="avail_day1"/>
    <field name="availDay2" type="integer" column="avail_day2"/>
    <field name="availDay3" type="integer" column="avail_day3"/>
    <field name="availDay4" type="integer" column="avail_day4"/>
    <field name="availDay5" type="integer" column="avail_day5"/>
    <field name="availDay6" type="integer" column="avail_day6"/>
    <field name="teamId1" type="integer" column="team_id1"/>
    <field name="teamId2" type="integer" column="team_id2"/>
    <field name="teamId3" type="integer" column="team_id3"/>
    <field name="modified" type="string" column="modified" length="16"/>
    <field name="notes" type="string" column="notes" length="1000"/>
    <lifecycle-callbacks/>
  </entity>
</doctrine-mapping>
