{% extends 'NatGamesBundle::layout.html.twig' %}

{% block title %}NatGames - Import{% endblock %}

{% block content %}
<div>
<form method="post" {{ form_enctype(form) }} action="{{ path('_natgames_admin_import') }}">
{{ form_widget(form.projectId) }}
<table border="1">
<tr><th colspan="2">Import Information</th></tr>
<tr>
  <td>{{ form_label (form.attachment) }}</td>
  <td>{{ form_widget(form.attachment) }}</td>
</tr>
<tr>
  <td>Processed File</td>
  <td>
    <input type="text" name="import_file_proc" readonly="readonly" size="40" value="Processed file name"/>
  </td>
</tr>
<tr>
  <td>.</td>
  <td align="center"><input type="submit" name="import_submit" value="Import" />
</tr>
<tr><td colspan="2">{{ form_errors(form.attachment) }}</td></tr>
</table>
{{ form_rest(form) }}
</form>
</div>
{% endblock %}
