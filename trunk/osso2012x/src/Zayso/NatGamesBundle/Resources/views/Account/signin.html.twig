{% form_theme form 'NatGamesBundle:Form:fields.html.twig' %}
{% extends 'NatGamesBundle::layout.html.twig' %}

{% block title %}NatGames - Account - Signin{% endblock %}

{% block content %}
<div>
<form action="{{ path('natgames_account_signin') }}" method="post" {{ form_enctype(form) }}>
<table border="1" style="width: 700px;">
<tr><th colspan="3">National Games 2012 Sign In</th></tr>
<tr>
  <td style="width: 100px;">{{ form_label (form.userName) }}</td>
  <td style="width: 200px;">{{ form_widget(form.userName) }}</td>
  <td style="width: 300px;">{{ form_errors(form.userName) }}</td>
</tr>
<tr>
  <td style="width: 100px;">{{ form_label (form.userPass) }}</td>
  <td style="width: 200px;">{{ form_widget(form.userPass) }}</td>
  <td style="width: 300px;">{{ form_errors(form.userPass) }}</td>
</tr>
<tr>
  <td>&nbsp;</td>
  <td align="center"><input type="submit" name="accountSigninSubmit" value="Sign In" />
  <td>&nbsp;</td>
</tr>
<tr><td colspan="3">{{ form_errors(form) }}</td></tr>
</table>
{{ form_rest(form) }}
</form>
<br />
</div>
{% endblock %}
