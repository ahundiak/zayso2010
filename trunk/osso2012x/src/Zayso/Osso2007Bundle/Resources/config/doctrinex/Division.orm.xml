<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Zayso\Osso2007Bundle\Entity\Division" table="division">
    <change-tracking-policy>DEFERRED_IMPLICIT</change-tracking-policy>
    <id name="divisionId" type="integer" column="division_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="sortx" type="integer" column="sortx"/>
    <field name="descPick" type="string" column="desc_pick" length="20"/>
    <field name="descLong" type="string" column="desc_long" length="20"/>
    <lifecycle-callbacks/>
  </entity>
</doctrine-mapping>
