<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Zayso\Osso2007Bundle\Entity\Field" table="field">
    <change-tracking-policy>DEFERRED_IMPLICIT</change-tracking-policy>
    <id name="fieldId" type="integer" column="field_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="fieldSiteId" type="integer" column="field_site_id"/>
    <field name="unitId" type="integer" column="unit_id"/>
    <field name="keyx" type="string" column="keyx" length="20"/>
    <field name="sortx" type="integer" column="sortx"/>
    <field name="descx" type="string" column="descx" length="40"/>
    <lifecycle-callbacks/>
  </entity>
</doctrine-mapping>
