<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Zayso\Osso2007Bundle\Entity\ProjectItem" table="project_item">
    <change-tracking-policy>DEFERRED_IMPLICIT</change-tracking-policy>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="projectId" type="integer" column="project_id"/>
    <field name="itemId" type="integer" column="item_id"/>
    <field name="typeId" type="integer" column="type_id"/>
    <field name="sort1" type="integer" column="sort1"/>
    <field name="status" type="integer" column="status"/>
    <field name="desc1" type="string" column="desc1" length="80"/>
    <lifecycle-callbacks/>
  </entity>
</doctrine-mapping>
