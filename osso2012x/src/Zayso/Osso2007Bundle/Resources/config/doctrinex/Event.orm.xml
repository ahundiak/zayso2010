<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Zayso\Osso2007Bundle\Entity\Event" table="event">
    <change-tracking-policy>DEFERRED_IMPLICIT</change-tracking-policy>
    <id name="eventId" type="integer" column="event_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="eventNum" type="integer" column="event_num"/>
    <field name="projectId" type="integer" column="project_id"/>
    <field name="regYearId" type="integer" column="reg_year_id"/>
    <field name="seasonTypeId" type="integer" column="season_type_id"/>
    <field name="scheduleTypeId" type="integer" column="schedule_type_id"/>
    <field name="eventTypeId" type="integer" column="event_type_id"/>
    <field name="classId" type="integer" column="class_id"/>
    <field name="unitId" type="integer" column="unit_id"/>
    <field name="status" type="integer" column="status"/>
    <field name="fieldId" type="integer" column="field_id"/>
    <field name="eventDate" type="string" column="event_date" length="8"/>
    <field name="eventTime" type="string" column="event_time" length="4"/>
    <field name="eventDuration" type="integer" column="event_duration"/>
    <field name="point1" type="integer" column="point1"/>
    <field name="point2" type="integer" column="point2"/>
    <lifecycle-callbacks/>
  </entity>
</doctrine-mapping>
