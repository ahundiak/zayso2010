<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Zayso\Osso2007Bundle\Entity\EventNote" table="event_note">
    <change-tracking-policy>DEFERRED_IMPLICIT</change-tracking-policy>
    <id name="eventNoteId" type="integer" column="event_note_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="eventId" type="integer" column="event_id"/>
    <field name="personId" type="integer" column="person_id"/>
    <field name="name" type="string" column="name" length="20"/>
    <field name="ts" type="string" column="ts" length="16"/>
    <field name="data1" type="string" column="data1" length="250"/>
    <lifecycle-callbacks/>
  </entity>
</doctrine-mapping>
